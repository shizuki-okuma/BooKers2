<%= flash[:notice] %>
<div class="container px-5 px-sm-0">
  <div class="container">
    <div class="row">
      <div class="col-md-3">
       
        <%= render 'books/list', user: @user %>
      </div>
        <div class="col-md-9 offser-md-1">
          <h2>Users</h2>
          <table class="table table-striped">
          <% @users.each do |user| %>
          <thead>
            <tr>
              <th>image</th><th>name</th><th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td><%= image_tag user.get_profile_image(100,100) %></td>
              <td><%= user.name %></td>
              <td><%= link_to "show", user_path(user.id) %></td>
            </tr>
          </tbody>
          <% end %>
        </table>
        </div>
      </div>
    </div>
  </div>
</div>


 